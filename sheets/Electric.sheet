  <contents>
    <object name="Electric - contact_o">
      <description xml:lang="fr">Un contact normalement ouvert</description>
      <description xml:lang="de">Ein normalerweise offener Kontakt</description>
      <description>A normally open contact</description>
    </object>
    <object name="Electric - contact_f">
      <description xml:lang="fr">Un contact normalement fermé</description>
      <description xml:lang="de">Ein normalerweise geschlossener Kontakt</description>
      <description>A normally closed contact</description>
    </object>
    <object name="Electric - intpos_o">
      <description xml:lang="fr">Un interrupteur de position normalement ouvert</description>
      <description xml:lang="de">Ein normalerweise offener Schalter</description>
