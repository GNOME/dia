    </object>
    <object name="Electric - intpos_f">
      <description xml:lang="fr">Un interrupteur de position normalement fermé</description>
      <description xml:lang="de">Ein normalerweise geschlossener Schalter</description>
      <description>A normally closed position switch</description>
    </object>
    <br/>
    <object name="Electric - command">
      <description xml:lang="fr">Organe de commande d'un relais</description>
      <description xml:lang="de">Der schaltende Teil eines Relais</description>
      <description>The command organ of a relay</description>
    </object>
    <object name="Electric - relay">
      <description xml:lang="fr">Un relais</description>
      <description xml:lang="de">Ein Relais</description>
      <description>A relay</description>
    </object>
    <object name="Electric - lamp">
      <description xml:lang="fr">Une lampe ou un voyant</description>
      <description xml:lang="de">Eine Lampe oder Indikatorleuchte</description>
      <description>A lamp or indicator light</description>
    </object>
    <br/>
    <object name="Electric - connpoint">
      <description xml:lang="fr">Un point de connexion</description>
      <description xml:lang="de">Ein Verbindungspunkt</description>
      <description>A connection point</description>
    </object>
    
