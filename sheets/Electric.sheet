<?xml version="1.0" encoding="iso-8859-1"?> <!-- -*- xml -*- -->

<sheet xmlns="http://www.lysator.liu.se/~alla/dia/dia-sheet-ns">
  <name>Electric</name>
  <name xml:lang="de">Stromkreis</name>
  <name xml:lang="hu">Elektromos áramkörök</name>
  <name xml:lang="fr">Electricité</name>
  <description xml:lang="fr">Composants de circuits électriques</description>
  <description xml:lang="de">Komponenten für Stromkreise</description>
  <description xml:lang="hu">Elektomos áramköri elemek</description>
