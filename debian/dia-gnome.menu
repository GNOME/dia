?package(dia-gnome): \
   needs="X11" \
   section="Applications/Graphics" \
   hints="Vector" \
   command="/usr/bin/dia" \
   icon="/usr/share/pixmaps/dia_menu.xpm" \
   title="Dia (GNOME)" \
   longtitle="Draw diagrams" \
   description="Dia can be used to draw different kind of diagrams. \
